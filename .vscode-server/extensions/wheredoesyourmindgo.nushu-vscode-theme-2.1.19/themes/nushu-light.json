        "foreground": "#0550ae"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#633c01"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2b2822"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2b2822"
      }
    },
    {
      "scope": [
        "markup.underline"
