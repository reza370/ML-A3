		"ts-node": "10.7.0",
		"typescript": "4.6.4",
		"util": "0.12.4",
		"vsce": "2.7.0",
		"webpack": "5.72.0",
		"webpack-cli": "4.9.2"
	},
	"galleryBanner": {
